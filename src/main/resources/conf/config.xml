<?xml version="1.0" encoding="UTF-8"?>
<config xmlns="http://www.leapframework.org/schema/config">
    <base-package>hello</base-package>

    <properties>
        <property name="db.driverClassName">com.mysql.jdbc.Driver</property>
        <property name="db.url">jdbc:mysql://localhost:3306/demo-sso?useUnicode=true</property>
        <property name="db.username">dev</property>
        <property name="db.password">123456</property>
    </properties>

    <properties prefix="oauth2">
        <property name="enabled"        value="true"/>
        <property name="server_url"     value="http://localhost:8088/"/>
        <property name="client_id"      value="client1"/>
        <property name="client_secret"  value="secret1"/>
    </properties>
</config>